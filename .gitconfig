[core]
	editor = vim
[rebase]
	autoSquash = true
[push]
	default = upstream
	recurseSubmodules = check
[merge]
	conflictstyle = diff3
[alias]
	st = status
	p = push
	pf = push --force
	l = log
	lf = log --format=fuller
	l1 = log --oneline
	c = commit
	d = diff
	dc = diff --cached
	dch = diff --cached HEAD~
	a = add
	f = fetch
	lo = log origin/master
	rtom = reset --hard origin/master
	sh = stash
	r = rebase
	mo = merge origin/master
	rf = reflog
	b = branch
	ba = branch -a
	cf = config
	rt = reset
	rt1 = reset HEAD~
	rth = reset --hard
	rc = rebase --committer-date-is-author-date
	ri = rebase -i
	rc1 = rebase HEAD~ --committer-date-is-author-date
	ck = checkout
	rom = rebase origin/master
	sm = submodule
	smd = !git diff && git submodule foreach 'git diff'
	smu = submodule update
	smf = submodule foreach
	sw = show
	swh = show HEAD
	swt = show --stat
	swth = show --stat HEAD
[status]
	submodulesummary = 1
